<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Pedro Tobo</Authors>
    <Company />
    <PackageProjectUrl>https://github.com/ptobo/JsonRPC-DotNet/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ptobo/JsonRPC-DotNet.git</RepositoryUrl>
